  "scripts": {
    "start": "cross-env ENV=web run-p build-tailwind start:app start:worker",
    "start:app": "react-app-rewired start",
    "start:worker": "nodemon --watch src -e js --exec \"npm run dev:worker\"",
    "dev:worker": "cra-append-sw --mode dev ./src/custom-service-worker.js",
    "build": "cross-env ENV=web run-s build-tailwind build:app build:worker build:tizon-cp",
    "build:tv": "cross-env ENV=tv run-s build-tailwind build:app build:worker build:tizon-cp",
    "build:app": "react-app-rewired build",
    "build:worker": "cra-append-sw ./src/custom-service-worker.js",
    "build-tailwind": "tailwindcss build src/tailwind.css -o src/tailwind.generated.css",
    "build:tizon": "cd build && ~/tizen-studio/tools/ide/bin/tizen package -t wgt && ~/tizen-studio/tools/ide/bin/tizen install --name AnimediaTV.wgt -- ./ && ~/tizen-studio/tools/ide/bin/tizen run -t UE55RU7172UXXH -p MAlXxWnEy4.AnimediaTV",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "figma": "node ./figma.js",
    "build:tizon-cp": "run-p build:tizon-cp:0 build:tizon-cp:1 build:tizon-cp:2 build:tizon-cp:3 build:tizon-cp:4 build:tizon-cp:5 build:tizon-cp:6 build:tizon-cp:7 build:tizon-cp:8",
    "build:tizon-cp:0": "ncp ./build/custom-service-worker.js ~/workspace/AnimediaTV/custom-service-worker.js",
    "build:tizon-cp:1": "ncp ./build/favicon.ico ~/workspace/AnimediaTV/favicon.ico",
    "build:tizon-cp:2": "ncp ./build/icon.png ~/workspace/AnimediaTV/icon.png",
    "build:tizon-cp:3": "ncp ./build/logo_appname_bg.png ~/workspace/AnimediaTV/logo_appname_bg.png",
    "build:tizon-cp:4": "ncp ./build/logo_appname_fg.png ~/workspace/AnimediaTV/logo_appname_fg.png",
    "build:tizon-cp:5": "ncp ./build/logo.svg ~/workspace/AnimediaTV/logo.svg",
    "build:tizon-cp:6": "ncp ./build/service-worker.js ~/workspace/AnimediaTV/service-worker.js",
    "build:tizon-cp:7": "ncp ./build/index.html ~/workspace/AnimediaTV/index.html",
    "build:tizon-cp:8": "ncp ./build/static ~/workspace/AnimediaTV/static"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cra-append-sw": "^2.7.0",
    "cross-env": "^7.0.2",
    "customize-cra": "^0.9.1",
    "figma-react": "^1.0.19",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "react-app-rewired": "^2.1.5",
    "styled-jsx": "^3.2.5",
    "tailwindcss": "^1.4.6"
  }
}
